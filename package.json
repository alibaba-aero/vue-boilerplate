{
  "name": "vuilerplate",
  "private": true,
  "version": "0.0.3",
  "scripts": {
    "build": "vue-tsc --noEmit && vite-ssg build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint:js": "eslint . --ext .js,.ts,.vue",
    "lint:style": "stylelint '**/*.{scss,css,vue}'",
    "lint": "pnpm lint:js && pnpm lint:style",
    "release:stg": "standard-version -s -a --prerelease beta && git push --follow-tags",
    "release:prod": "standard-version -s -a && git push --follow-tags",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "cypress open",
    "prepare": "husky install",
    "typecheck": "vue-tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --ext .js,.ts,.vue --fix",
      "pnpm vitest related --run"
    ],
    "*.{scss,vue}": "stylelint --fix"
  },
  "dependencies": {
    "@vueuse/core": "8.9.4",
    "@vueuse/head": "0.7.7",
    "@vueuse/schema-org": "0.6.5",
    "pinia": "2.0.16",
    "prism-theme-vars": "^0.2.3",
    "vue": "3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "4.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/vite-plugin-vue-i18n": "5.0.1",
    "@pinia/testing": "0.0.12",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/vue": "6.6.1",
    "@typescript-eslint/parser": "5.31.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/test-utils": "2.0.2",
    "c8": "7.12.0",
    "cypress": "^10.3.0",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-vue": "9.2.0",
    "happy-dom": "6.0.4",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-prism": "^2.2.4",
    "postcss": "8.4.14",
    "postcss-html": "1.5.0",
    "postcss-import": "14.1.0",
    "postcss-nested": "5.0.6",
    "postcss-url": "10.1.3",
    "rollup-plugin-visualizer": "5.7.1",
    "standard-version": "9.5.0",
    "stylelint": "14.9.1",
    "stylelint-config-recommended-vue": "1.4.0",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.9.4",
    "unplugin-vue-components": "0.21.2",
    "vite": "3.0.2",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-md": "0.19.7",
    "vite-plugin-pages": "0.25.0",
    "vite-plugin-pwa": "0.12.3",
    "vite-plugin-vue-layouts": "0.6.0",
    "vite-ssg": "^0.20.2",
    "vitest": "0.18.1",
    "vue-eslint-parser": "9.0.3",
    "vue-tsc": "0.39.0"
  }
}
